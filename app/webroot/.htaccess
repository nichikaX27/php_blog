<!-- サーバ環境用 -->
<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /blog/cakephp-2.10.24/app/webroot/
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php [QSA,L]
</IfModule>